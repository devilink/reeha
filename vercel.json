{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static"
        },
        {
            "src": "api/products.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/products",
            "dest": "/api/products.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}